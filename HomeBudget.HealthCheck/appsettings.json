{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Directories Service",
        "Uri": "http://localhost:5002/health"
      },
      {
        "Name": "Auth Service",
        "Uri": "http://localhost:5001/health"
      }
    ],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 20
  },
  "AllowedHosts": "*"
}
